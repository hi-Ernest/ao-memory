# OpenAI API 配置
OPENAI_API_KEY=sk-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENAI_MODEL=gpt-4o-mini

# Ollama 配置 (用于本地嵌入模型)
OLLAMA_URL=http://localhost:11434

# Weaviate 向量数据库配置
WEAVIATE_URL=http://localhost:8080
WEAVIATE_API_KEY=

# AO 网络配置
# 注意：AO_WALLET_JSON 应该是一个有效的 JSON 字符串
AO_WALLET_JSON={"kty":"RSA","n":"your_wallet_private_key_here","e":"AQAB","d":"your_private_exponent","p":"your_p_value","q":"your_q_value","dp":"your_dp_value","dq":"your_dq_value","qi":"your_qi_value"}

# AO 进程 ID 配置
MEMORY_PROCESS_ID=your_memory_process_id_here
MARKET_PROCESS_ID=your_market_process_id_here

# Gateway 服务器配置
PORT=8787

# 可选：如果 Weaviate 需要认证
# WEAVIATE_API_KEY=your_weaviate_api_key_if_needed
